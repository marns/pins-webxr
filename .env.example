# Signaling endpoint (full URL to POST /offer)
# VITE_OFFER_URL=http://localhost:8080/offer

# ICE servers (optional). JSON array of RTCIceServer objects only.
# VITE_ICE_SERVERS=[{"urls":"stun:stun.l.google.com:19302"}]

# Video crop (optional). Fractions in [0,1]. Defaults shown.
# Crop is centered at (center_x, center_y) with size (width_pct x height_pct).
# VITE_CROP_CENTER_X_PCT=0.5
# VITE_CROP_CENTER_Y_PCT=0.5
# VITE_CROP_WIDTH_PCT=1
# VITE_CROP_HEIGHT_PCT=1
